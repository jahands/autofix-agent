{
	"name": "@repo/autofix",
	"version": "0.1.0",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "run-vite-build",
		"build:container": "docker build --platform linux/amd64 --tag autofix-container:$(git rev-parse --short HEAD) -f ./src/container-server/Dockerfile-docker ../../ && wrangler containers push autofix-container:$(git rev-parse --short HEAD)",
		"build:container:simple": "docker build --platform linux/amd64 --tag simple-container:master -f ./src/container-server/Dockerfile-docker-simple ../../ && wrangler containers push simple-container:master",
		"check:lint": "run-eslint-default",
		"check:types": "run-tsc",
		"deploy": "run-wrangler-deploy",
		"dev": "run-vite-dev",
		"fix:workers-types": "run-wrangler-types",
		"preview": "run-vite-preview",
		"start:container": "tsx src/container/container.app.ts",
		"test": "run-vitest"
	},
	"dependencies": {
		"@hono/node-server": "1.14.1",
		"@hono/standard-validator": "0.1.2",
		"@hono/zod-validator": "0.5.0",
		"@octokit/rest": "21.1.1",
		"@repo/hono-helpers": "workspace:*",
		"agents": "0.0.87",
		"ai": "4.3.10",
		"hono": "4.7.8",
		"itty-time": "2.0.1",
		"mime": "4.0.7",
		"mock-fs": "5.5.0",
		"ts-pattern": "5.7.1",
		"tsx": "4.19.4",
		"workers-ai-provider": "0.4.1",
		"workers-tagged-logger": "0.11.2",
		"zod": "3.25.7"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "1.1.0",
		"@cloudflare/vitest-pool-workers": "0.8.30",
		"@repo/eslint-config": "workspace:*",
		"@repo/tools": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@types/mock-fs": "4.13.4",
		"@types/node": "22.15.19",
		"vite": "6.3.4",
		"vitest": "3.1.3",
		"wrangler": "4.16.0"
	}
}
